{
  "compilerOptions": {
    "target": "esnext",
    "module": "esnext",
    "moduleResolution": "node",
    "esModuleInterop": true,
    "allowJs": true,
    "allowSyntheticDefaultImports": true,
    "isolatedModules": true,
    "declaration": true,
    "strict": true,
    "outDir": "dist",
    "lib": ["esnext"],
    "resolveJsonModule": true,
    "types": ["@cloudflare/workers-types"]
  }
}
